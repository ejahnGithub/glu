struct Point {
    x: Int,
    y: Int
}

func add(a: Int, b: Int) -> Int {
    return a + b;
}

func main() -> Int {
    // Type mismatch: trying to assign integer to struct
    let p: Point = 42;
    
    // Function call with wrong argument types
    let result = add("hello", 3.14);
    
    // Another type mismatch with float
    let num: Int = 3.14159;
    
    // Variable assignment type mismatch
    var x: Bool = "not a boolean";
    
    return 0;
}