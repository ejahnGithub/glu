import file::writeFile;

@no_mangling func puts(s: *Char);
@no_mangling func getchar() -> Char;
@no_mangling func strcat(destination: *Char, source: *Char) -> *Char;
@no_mangling func putchar(c: Char) -> Int;

public func print(s: String) -> Void {
    writeFile({1}, s.data, s.length);
}

public func printLine(s: String) -> Void {
    print(s);
    print("\n");
}

public func getCharacter() -> Char {
    return getchar();
}

public func printCharacter(c: Char) -> Int {
    return putchar(c);
}
