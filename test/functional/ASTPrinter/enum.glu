//
// RUN: gluc --print-ast %s | FileCheck -v %s
//

// CHECK:ModuleDecl {{0x[0-9a-f]+}} <{{.*}}enum.glu, line:1:1>
// CHECK-NEXT:  -->Decls:
// CHECK:    EnumDecl {{0x[0-9a-f]+}} <line:11:1>
// CHECK-NEXT:      -->Name: E
// CHECK:      -->Name: A
// CHECK:      -->Name: B
enum E: Int { A, B = 2 }

// CHECK:    EnumDecl {{0x[0-9a-f]+}} <line:18:1>
// CHECK-NEXT:      -->Name: MyEnum
// ## TODO: This should be 1 and 2, but = is not supported yet.
// CHECK:      -->Name: CASE1
// CHECK:      -->Name: CASE2
enum MyEnum: Int { CASE1 = 1, CASE2 = 2 }
