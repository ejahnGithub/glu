//
// RUN: gluc --print-ast %s | FileCheck -v %s
//

// CHECK:ModuleDecl <struct.glu, line:1:2>
// CHECK:  -->Decls:
// CHECK:    StructDecl <line:12:1>
// CHECK:      -->Name: S
// CHECK:      -->Fields:
// CHECK:      |  a : UnresolvedName
// CHECK:      |  b : UnresolvedName
struct S {
    a: Int,
    b: Int,
}

// CHECK:    TypeAliasDecl <line:20:11>
// CHECK:      -->Name: Point
// CHECK:      -->Type: UnresolvedName
typealias Point = S;

// CHECK:    TypeAliasDecl <line:26:11>
// CHECK:      -->Name: MyAlias
// ## TODO: This should be Int, but not supported yet.
// CHECK:      -->Type: UnresolvedName
typealias MyAlias = Int;
