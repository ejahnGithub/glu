//
// RUN: gluc --print-ast %s | FileCheck -v %s
//

// CHECK:ModuleDecl {{0x[0-9a-f]+}} <{{.*}}struct.glu, line:1:1>
// CHECK:  -->Decls:
// CHECK-NEXT:    StructDecl {{0x[0-9a-f]+}} <line:16:1>
// CHECK-NEXT:      -->Name: S
// CHECK-NEXT:      -->Fields:
// CHECK-NEXT:        FieldDecl {{0x[0-9a-f]+}} <line:17:5>
// CHECK-NEXT:          -->Name: a
// CHECK-NEXT:          -->Type: i32
// CHECK-NEXT:        FieldDecl {{0x[0-9a-f]+}} <line:18:5>
// CHECK-NEXT:          -->Name: b
// CHECK-NEXT:          -->Type: i32
struct S {
    a: Int,
    b: Int,
}

// CHECK-NEXT:    TypeAliasDecl {{0x[0-9a-f]+}} <line:24:11>
// CHECK-NEXT:      -->Name: Point
// CHECK-NEXT:      -->Type:
typealias Point = S;

// CHECK-NEXT:    TypeAliasDecl {{0x[0-9a-f]+}} <line:30:11>
// CHECK-NEXT:      -->Name: MyAlias
// ## TODO: This should be Int, but not supported yet.
// CHECK-NEXT:      -->Type: i32
typealias MyAlias = Int;
