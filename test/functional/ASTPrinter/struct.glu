//
// RUN: gluc --print-ast %s | FileCheck -v %s
//

// CHECK:ModuleDecl {{0x[0-9a-f]+}} <{{.*}}struct.glu, line:1:1>
// CHECK:  -->Decls:
// CHECK-NEXT:    StructDecl {{0x[0-9a-f]+}} <line:16:1>
// CHECK-NEXT:      -->Name: S
// CHECK:      -->Fields:
// CHECK-NEXT:        FieldDecl {{0x[0-9a-f]+}} <line:17:5>
// CHECK-NEXT:          -->Name: a
// CHECK-NEXT:          -->Type: Int32
// CHECK:             FieldDecl {{0x[0-9a-f]+}} <line:18:5>
// CHECK-NEXT:          -->Name: b
// CHECK-NEXT:          -->Type: Int32
struct S {
    a: Int,
    b: Int,
}

// CHECK:         TypeAliasDecl {{0x[0-9a-f]+}} <line:24:11>
// CHECK-NEXT:      -->Name: Point
// CHECK-NEXT:      -->Type: S
typealias Point = S;

// CHECK:    TypeAliasDecl {{0x[0-9a-f]+}} <line:29:11>
// CHECK-NEXT:      -->Name: MyAlias
// CHECK-NEXT:      -->Type: Int32
typealias MyAlias = Int;
