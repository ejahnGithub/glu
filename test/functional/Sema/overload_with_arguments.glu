// Define overloaded functions with different parameter types
func process(x: Int) -> String {
    return "processed int";
}

func process(x: Float) -> String {
    return "processed float";
}

func process(x: String) -> Int {
    return 42;
}

func main() -> Int {
    // Test case 1: Ambiguous call with literal that could be int or float
    let result1 = process(42);
    // CHECK: note: process(f32) -> String
    // CHECK: note: process(i32) -> String
    // CHECK: error: Ambiguous type variable mapping found: multiple valid solutions exist
    // CHECK: note: Consider adding explicit type annotations to resolve the ambiguity
    // CHECK: note: Could resolve as:
    
    // Test case 2: Wrong argument type - should show available overloads
    let result2 = process(true);
    // CHECK: error: Cannot convert (Bool) -> ? to type inferred from function call to 'process'
    // CHECK: note: Available overloads:
    // CHECK: note: process(i32) -> String
    // CHECK: note: process(f32) -> String 
    // CHECK: note: process(String) -> i32
    
    return 0;
}