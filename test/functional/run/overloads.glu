//
// RUN: gluc %s -o %t && %t | FileCheck -v %s
//

@no_mangling func puts(s: *Char);

func overload(s: *Char) -> Void {
    puts("This is a String");
}

func overload(n: Int) -> Void {
    puts("This is an Int");
}

// CHECK: This is a String
// CHECK: This is an Int
func main() -> Int {
    overload("String");
    overload(42);
    return 0;
}
