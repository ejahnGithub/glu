struct S {
    a: Int,
    public b: Int,
}

enum E {
    A,
    B,
    C,
}

func foo(s: S, @unused e: E) -> Int {
    let result = s.a + s.b;
    return result;
}

@linkage_name("borabora")
public func bar() -> Void {
    // do nothing
}

