struct S {
    a: Int,
    b: Int,
}

enum E {
    A,
    B,
    C,
}

func foo(s: S, e: E) -> Int {
    let result = s.a + s.b + e as Int;
    return result;
}

public func bar() -> Void {
    // do nothing
}

